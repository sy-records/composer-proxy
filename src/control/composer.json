{
  "name": "simple-swoole/skeleton",
  "type": "project",
  "description": "A simple, lightweight and high-performance PHP coroutine framework.",
  "keywords": [
    "php",
    "swoole",
    "coroutine",
    "simple",
    "simps"
  ],
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "ext-swoole": ">=4.4",
    "simple-swoole/simps": "~1.0.0",
    "simple-swoole/db": "~1.0.0",
    "smi2/phpclickhouse": "dev-master",
    "kkgroup/validation": "^0.2.0",
    "symfony/yaml": "^7.2",
    "ext-redis": "*",
    "hyperf/coroutine": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/functions.php"
    ]
  },
  "bin": [
    "bin/simps.php"
  ],
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "start": "php bin/simps.php http:start"
  }
}
